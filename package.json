{
  "name": "c2-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@ant-design/icons": "^4.8.0",
    "@loadable/component": "^5.15.2",
    "@reduxjs/toolkit": "^1.7.1",
    "@types/lodash": "^4.14.191",
    "@types/moment": "^2.13.0",
    "@types/node": "^16.7.13",
    "@types/react": "^17.0.20",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.3.3",
    "antd": "^5.0.1",
    "antd-img-crop": "^4.5.2",
    "antd-mask-input": "^2.0.7",
    "axios": "^0.25.0",
    "cross-env": "^7.0.3",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "react": "^17.0.2",
    "react-cropper": "^2.1.8",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.6",
    "react-router-dom": "6",
    "react-scripts": "5.0.0",
    "react-toastify": "^9.1.1",
    "sass": "^1.56.1",
    "styled-components": "^5.3.3",
    "typescript": "^4.4.2"
  },
  "scripts": {
    "dev": "cross-env env-cmd -f .env.development react-scripts start",
    "dev:staging": "env-cmd -f .env.staging react-scripts start",
    "dev:production": "env-cmd -f .env.production react-scripts start",
    "build:dev": "cross-env GENERATE_SOURCEMAP=false env-cmd -f .env.development react-scripts build",
    "build:staging": "cross-env GENERATE_SOURCEMAP=false env-cmd -f .env.staging react-scripts build",
    "build:production": "cross-env GENERATE_SOURCEMAP=false env-cmd -f .env.production react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@storybook/addon-actions": "^6.4.14",
    "@storybook/addon-essentials": "^6.4.14",
    "@storybook/addon-links": "^6.4.14",
    "@storybook/builder-webpack5": "^6.4.14",
    "@storybook/manager-webpack5": "^6.4.14",
    "@storybook/node-logger": "^6.4.14",
    "@storybook/preset-create-react-app": "^4.0.0",
    "@storybook/react": "^6.4.14",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.0",
    "@types/loadable__component": "^5.13.4",
    "@types/styled-components": "^5.1.21",
    "@typescript-eslint/eslint-plugin": "^5.10.0",
    "@typescript-eslint/parser": "^5.10.0",
    "babel-loader": "^8.2.3",
    "env-cmd": "^10.1.0",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-storybook": "^0.5.6",
    "husky": "^7.0.0",
    "i18next": "^21.6.7",
    "prettier": "^2.5.1",
    "pretty-quick": "^3.1.3",
    "react-i18next": "^11.15.3",
    "webpack": "5"
  }
}
